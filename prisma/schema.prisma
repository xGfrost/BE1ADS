generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model cuti {
  id           Int       @id @default(autoincrement())
  Nomor_Induk  String?   @db.VarChar(10)
  Tanggal_Cuti DateTime? @db.Date
  Lama_Cuti    Int?
  Keterangan   String?   @db.VarChar(50)
  karyawan     karyawan? @relation(fields: [Nomor_Induk], references: [Nomor_Induk], onDelete: Restrict, onUpdate: Restrict, map: "cuti_ibfk_1")

  @@index([Nomor_Induk], map: "Nomor_Induk")
}

model karyawan {
  Nomor_Induk       String    @id @db.VarChar(10)
  Nama              String?   @db.VarChar(50)
  Alamat            String?   @db.VarChar(100)
  Tanggal_Lahir     DateTime? @db.Date
  Tanggal_Bergabung DateTime? @db.Date
  cuti              cuti[]
}

